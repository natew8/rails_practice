<p id="notice"><%= notice %></p>
<div class="jumbotron">
  <h1 class="display-4">
  <%= @friend.first_name %> 
  <%= @friend.last_name %>
  </h1>
  <p class="lead">
  <%= @friend.email %> | <%= @friend.phone_number %> | <%= @friend.twitter %>
  </p>
  <hr class="my-4">
  <br/>
  <p class="lead">
    <%= link_to 'Edit', edit_friend_path(@friend), class: "btn btn-secondary" %> 
    <%= link_to 'Back', friends_path, class: "btn btn-secondary" %>
  </p>
</div>
